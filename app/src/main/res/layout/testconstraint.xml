<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="familyconnect.familyconnect.GroupedActivities$PlaceholderFragment"
    tools:layout_editor_absoluteY="81dp">


    <ImageView
        android:id="@+id/grayBGimage"
        android:layout_width="411dp"
        android:layout_height="603dp"
        android:background="#ebebeb"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"></ImageView>


    <ListView
        android:id="@+id/activityScroll"
        android:layout_width="379dp"
        android:layout_height="350dp"
        android:text="Loading Activities..."
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="159dp">


    </ListView>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Activity"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="57dp" />


</android.support.constraint.ConstraintLayout>

